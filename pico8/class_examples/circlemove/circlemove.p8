pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- circlemove
-- by juanje

function _init()
 cls()
 x=64
 y=64
 speed=3
 radius=5
 clr=0 --color
end


function _update()
 if btn(0) then
  x=x-speed
  x=mid(radius,x,127)
 end
 
 if btn(1) then
  x=x+speed
  x=mid(0,x,127-radius)
 end
 
 if btn(2) then
  y=y-speed
  y=mid(0,y,127)
 end
 
 if btn(3) then
  y=y+speed
  y=mid(0,y,127)
 end
 
 clr=clr+1
end


function _draw()
 cls()
 pset(x,y,3)
 circ(x,y,radius,clr)
end
