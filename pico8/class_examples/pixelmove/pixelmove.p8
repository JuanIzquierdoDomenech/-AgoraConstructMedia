pico-8 cartridge // http://www.pico-8.com
version 18
__lua__


function _init()
 cls()
 x=64
 y=0
-- dy=0
-- g=1
end

function _update()

 -- move pixel right
 if btn(1) then
  x=x+1
 end
 
 -- move pixel up
 if btn(2) then
  y=y-1
 end
 
 -- move pixel left
 if btn(0) then
  x=x-1
 end
 
 -- move pixel down
 if btn(3) then
  y=y+1
 end
 
-- dy=dy+1
-- y=y+dy
end

function _draw()
 cls()
 pset(x,y,9)
end
